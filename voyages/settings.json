{
    "[python]": {
        "editor.formatOnType": true,
        "diffEditor.ignoreTrimWhitespace": true,
        "editor.formatOnSave": true,
        "python.formatting.provider": "black",
        "python.linting.flake8Enabled": true,
        "python.linting.flake8Args": [
            "--max-line-length=88"
        ],
        "python.linting.flake8Path": "${workspaceFolder}/venv/Scripts/flake8.exe",
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "files.autoSave": "afterDelay",
    "CodeGPT.query.language": "Russian",
    "workbench.preferredDarkColorTheme": "Tomorrow Night Blue",
    "workbench.editorAssociations": {
        "{git,gitlens,conflictResolution,vscode-local-history}:/**/*.tc.json": "default",
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.txt": "default",
        "*.xlsx": "default",
        "*.db": "default",
        "*.html": "default"
    },
    "jupyter.interactiveWindow.creationMode": "perFile",
    "editor.inlineSuggest.suppressSuggestions": true,
    "tabby.inlineCompletion.triggerMode": "automatic",
    "cody.autocomplete.enabled": false,
    "interactiveWindow.executeWithShiftEnter": true,
    "GPTutor.outputLanguage": "Russian",
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    "editor.inlineSuggest.showToolbar": "always",
    "git.autofetch": true,
    "amazonQ.telemetry": false,
    "vscode-edge-devtools.webhint": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.productIconTheme": "developer-icons",
    "python.analysis.autoImportCompletions": true,
    "python.autoComplete.extraPaths": [],
    "git.ignoreRebaseWarning": true,
    "json.schemas": [],
    "php.validate.executablePath": ""
}